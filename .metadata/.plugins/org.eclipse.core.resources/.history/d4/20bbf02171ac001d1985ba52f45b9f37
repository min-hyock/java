package 자료;

import java.util.Scanner;

public class javafestival2_2번 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		while (true) {
			System.out.print(" A 입력 >> ");
			int a = sc.nextInt();
			System.out.print(" B 입력 >> ");
			int b = sc.nextInt();
			int minus = a - b;
			if (a == 0 && b == 0) {
				System.out.println("결과 >> " + minus);
				break;
			}
		}

	}

}
