package while문;

import java.util.Scanner;

public class ex02while문예제 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.print("정수 입력 : ");
		int num = sc.nextInt();
		
		while(num<10) {
			System.out.println("정수 입력 : ");
		}
		
		System.out.println("종료되었습니다");
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
