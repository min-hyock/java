package 입출력;

public class ex01입출력 {

	public static void main(String[] args) {
		// 주석 : 코드에 영향을 끼치지 않는 일종의 메모
		// 단축키
		// 1) 글자 크기 늘리기 : ctrl + +
		// 2) 글자 크기 줄이기 : ctrl + -
		// 3) 한 줄 삭제 : ctrl + d
		
		// 출력문
		// ---> () 안쪽에 출력하고 싶은 구문 작성
		System.out.println("Hello World!!!!");
		System.out.println("Hello World!!!!");
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
